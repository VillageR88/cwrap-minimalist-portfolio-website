            {
                "element": "span",
                "style": "max-width: 152px; width: 100%; font-family: Public Sans, serif; font-size: 12px; letter-spacing: 2px; text-align: center; text-decoration: none;",
                "text": "cwrapProperty[buttonName=ABOUT ME]"
            }
        ]
    }
