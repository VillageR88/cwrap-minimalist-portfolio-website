{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "../../favicon.ico",
        "type": "image/x-icon"
      }
    ]
  },
  "routes": [
    {
      "route": "manage"
    },
    {
      "route": "bookmark"
    },
    {
      "route": "insure"
    },
    {
      "route": "fylo"
    }
  ],
  "element": "cwrap-template",
  "omit": [
    "omitScript"
  ],
  "text": "cwrapTemplate[bodyUniversal(color2=#5FB4A2,prefix=../../)]",
  "passover": [
    {
      "element": "main",
      "mediaQueries": [
        {
          "query": "max-width: 1024px",
          "style": "margin-block: 80px;"
        }
      ],
      "style": "margin-block: 94px 150px; margin-inline: auto; max-width: 1110px;",
      "children": [
        {
          "element": "picture",
          "children": [
            {
              "element": "source",
              "attributes": {
                "media": "(max-width: 480px)",
                "srcset": "../../static/images/detail/mobile/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero.jpg 1x, ../../static/images/detail/mobile/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero@2x.jpg 2x"
              }
            },
            {
              "element": "source",
              "attributes": {
                "media": "(max-width: 768px)",
                "srcset": "../../static/images/detail/tablet/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero.jpg 1x, ../../static/images/detail/tablet/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero@2x.jpg 2x"
              }
            },
            {
              "element": "img",
              "style": "width: 100%;",
              "attributes": {
                "src": "../../static/images/detail/desktop/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero.jpg",
                "srcset": "../../static/images/detail/desktop/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero.jpg 1x, ../../static/images/detail/desktop/image-cwrapRoutes[manage,bookmark,insure,fylo]-hero@2x.jpg 2x"
              }
            }
          ]
        },
        {
          "element": "div",
          "mediaQueries": [
            {
              "query": "max-width: 1024px",
              "style": "flex-direction: column; margin-top: 40px;"
            }
          ],
          "style": "display: flex; gap: 32px; justify-content: space-between; margin-top: 115px;",
          "children": [
            {
              "element": "div",
              "mediaQueries": [
                {
                  "query": "max-width: 1024px",
                  "style": "max-width: unset; padding-block: 32px;"
                }
              ],
              "style": "border-block: 1px solid rgba(51, 50, 61, 0.15); display: flex; flex-direction: column; height: fit-content; justify-content: center; max-width: 350px; padding-block: 48px;",
              "children": [
                {
                  "element": "section",
                  "style": "display: flex; flex-direction: column; gap: 29px;",
                  "children": [
                    {
                      "element": "h2",
                      "attributes": {
                        "class": "title"
                      },
                      "text": "cwrapRoutes[Manage,Bookmark,Insure,Fylo]"
                    },
                    {
                      "element": "p",
                      "mediaQueries": [
                        {
                          "query": "max-width: 1024px",
                          "style": "max-width: 339px; position: absolute; right: 32px;"
                        },
                        {
                          "query": "max-width: 768px",
                          "style": "max-width: unset; position: unset; right: unset;"
                        }
                      ],
                      "style": "font-size: 15px;",
                      "attributes": {
                        "class": "description"
                      },
                      "text": "cwrapRoutes[cwrapGlobal[manageDescription] , cwrapGlobal[bookmarkDescription] , cwrapGlobal[insureDescription] , cwrapGlobal[fyloDescription]]"
                    }
                  ]
                },
                {
                  "element": "div",
                  "style": "display: flex; flex-direction: column; gap: 16px; margin-top: 23px;",
                  "children": [
                    {
                      "element": "p",
                      "text": "cwrapGlobal[description1]",
                      "attributes": {
                        "class": "text1"
                      }
                    },
                    {
                      "element": "p",
                      "text": "cwrapRoutes[cwrapGlobal[description2Long],cwrapGlobal[description2Long],cwrapGlobal[description2Long],cwrapGlobal[description2Short]]",
                      "attributes": {
                        "class": "text1"
                      }
                    }
                  ]
                },
                {
                  "element": "a",
                  "style": "margin-top: 32px; max-width: 178px;",
                  "text": "VISIT WEBSITE",
                  "attributes": {
                    "class": "button2"
                  }
                }
              ]
            },
            {
              "element": "div",
              "mediaQueries": [
                {
                  "query": "max-width: 1024px",
                  "style": "max-width: unset;"
                }
              ],
              "style": "display: flex; flex-direction: column; gap: 40px; max-width: 635px; width: 100%;",
              "children": [
                {
                  "element": "section",
                  "style": "display: flex; flex-direction: column; gap: 28px;",
                  "children": [
                    {
                      "element": "h3",
                      "attributes": {
                        "class": "titleNarrow"
                      },
                      "text": "Project Background"
                    },
                    {
                      "element": "p",
                      "style": "font-size: 15px;",
                      "attributes": {
                        "class": "description"
                      },
                      "text": "This project was a front-end challenge from cwrapSpan. It’s a platform that enables you to practice building websites to a design and project brief. Each challenge includes mobile and desktop designs to show how the website should look at different screen sizes. Creating these projects has helped me refine my workflow and solve real-world coding problems. I’ve learned something new with each project, helping me to improve and adapt my style.",
                      "children": [
                        {
                          "element": "a",
                          "style": "text-decoration-color: rgba(51, 50, 61, 0.8); text-decoration-thickness: 1.3px; text-underline-offset: 2px;",
                          "text": "Frontend Mentor",
                          "attributes": {
                            "href": "https://www.frontendmentor.io/home",
                            "class": "description"
                          }
                        }
                      ]
                    }
                  ]
                },
                {
                  "element": "div",
                  "style": "display: flex; flex-direction: column; gap: 28px;",
                  "children": [
                    {
                      "element": "h3",
                      "attributes": {
                        "class": "titleNarrow"
                      },
                      "text": "Static Previews"
                    },
                    {
                      "element": "ul",
                      "style": "display: flex; flex-direction: column; gap: 32px; list-style: none; margin: 0; padding: 0;",
                      "blueprint": {
                        "count": "2",
                        "element": "li",
                        "children": [
                          {
                            "element": "picture",
                            "children": [
                              {
                                "element": "source",
                                "attributes": {
                                  "media": "(max-width: 480px)",
                                  "srcset": "../../static/images/detail/mobile/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2].jpg 1x, ../../static/images/detail/mobile/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2]@2x.jpg 2x"
                                }
                              },
                              {
                                "element": "source",
                                "attributes": {
                                  "media": "(max-width: 1024px)",
                                  "srcset": "../../static/images/detail/tablet/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2].jpg 1x, ../../static/images/detail/tablet/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2]@2x.jpg 2x"
                                }
                              },
                              {
                                "element": "img",
                                "style": "width: 100%;",
                                "attributes": {
                                  "src": "../../static/images/detail/desktop/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2].jpg",
                                  "srcset": "../../static/images/detail/desktop/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2].jpg 1x, ../../static/images/detail/desktop/image-cwrapRoutes[manage,bookmark,insure,fylo]-preview-cwrapArray[1,2]@2x.jpg 2x"
                                }
                              }
                            ]
                          }
                        ]
                      }
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "element": "div",
          "mediaQueries": [
            {
              "query": "max-width: 768px",
              "style": "height: 148px;"
            }
          ],
          "style": "align-items: center; border-block: 1px solid rgba(51, 50, 61, 0.15); display: flex; height: 132px; justify-content: space-between; margin-top: 64px; width: 100%;",
          "children": [
            {
              "element": "cwrap-template",
              "text": "cwrapTemplate[projectTransition(animation=moveRightLeft,transitionTitle=cwrapRoutes[Fylo,Manage,Bookmark,Insure],transitionLink=cwrapRoutes[fylo,manage,bookmark,insure],direction=Previous,alignQueryArrow=start)]"
            },
            {
              "element": "div",
              "style": "border-left: 1px solid rgba(51, 50, 61, 0.15); height: 100%;"
            },
            {
              "element": "cwrap-template",
              "text": "cwrapTemplate[projectTransition(animation=moveLeftRight,transitionTitle=cwrapRoutes[Bookmark,Insure,Fylo,Manage],transitionLink=cwrapRoutes[bookmark,insure,fylo,manage],direction=Next,alignTransition=end,transitionRowDirection=row-reverse,arrowRotation=180,alignQueryArrow=end)]"
            }
          ]
        }
      ]
    }
  ]
}