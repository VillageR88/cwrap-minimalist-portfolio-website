{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "../favicon.ico",
        "type": "image/x-icon"
      },
      {
        "rel": "prefetch",
        "href": "../portfolio"
      },
      {
        "rel": "prefetch",
        "href": "../portfolio/manage"
      },
      {
        "rel": "prefetch",
        "href": "../portfolio/bookmark"
      },
      {
        "rel": "prefetch",
        "href": "../portfolio/insure"
      },
      {
        "rel": "prefetch",
        "href": "../portfolio/fylo"
      },
      {
        "rel": "prefetch",
        "href": "../contact-me"
      }
    ]
  },
  "element": "cwrap-template",
  "text": "cwrapTemplate[bodyUniversal(color2=#5FB4A2,prefix=../)]",
  "passover": [
    {
      "element": "main",
      "mediaQueries": [
        {
          "query": "max-width: 920px",
          "style": "margin-bottom: 96px;"
        },
        {
          "query": "max-width: 768px",
          "style": "margin-block: 40px 80px;"
        }
      ],
      "style": "display: flex; flex-direction: column; margin-block: 94px 150px; margin-inline: auto; max-width: 1110px; width: 100%;",
      "children": [
        {
          "element": "ul",
          "extend": [
            {
              "extension": " > li:nth-of-type(even)",
              "style": "align-self: end; flex-direction: row-reverse;"
            }
          ],
          "style": "display: flex; flex-direction: column; gap: 80px; list-style: none; margin: 0; padding: 0;",
          "blueprint": {
            "count": "4",
            "element": "li",
            "mediaQueries": [
              {
                "query": "max-width: 920px",
                "style": "gap: 32px;"
              },
              {
                "query": "max-width: 768px",
                "style": "flex-direction: column; gap: 32px; max-width: unset;"
              }
            ],
            "style": "align-items: center; display: flex; gap: 64px; justify-content: space-between; max-width: 1015px; width: 100%;",
            "children": [
              {
                "element": "picture",
                "mediaQueries": [
                  {
                    "query": "max-width: 768px",
                    "style": "max-width: 400px; width: 100%;"
                  }
                ],
                "children": [
                  {
                    "element": "source",
                    "attributes": {
                      "media": "(max-width: 768px)",
                      "srcset": "../static/images/portfolio/mobile/image-portfolio-cwrapArray[manage,bookmark,insure,fylo].jpg 1x, ../static/images/portfolio/mobile/image-portfolio-cwrapArray[manage,bookmark,insure,fylo]@2x.jpg 2x"
                    }
                  },
                  {
                    "element": "source",
                    "attributes": {
                      "media": "(max-width: 920px)",
                      "srcset": "../static/images/portfolio/tablet/image-portfolio-cwrapArray[manage,bookmark,insure,fylo].jpg 1x, ../static/images/portfolio/tablet/image-portfolio-cwrapArray[manage,bookmark,insure,fylo]@2x.jpg 2x"
                    }
                  },
                  {
                    "element": "img",
                    "style": "display: flex; width: 100%;",
                    "attributes": {
                      "src": "../static/images/portfolio/desktop/image-portfolio-cwrapArray[manage,bookmark,insure,fylo].jpg",
                      "srcset": "../static/images/portfolio/desktop/image-portfolio-cwrapArray[manage,bookmark,insure,fylo].jpg 1x, ../static/images/portfolio/desktop/image-portfolio-cwrapArray[manage,bookmark,insure,fylo]@2x.jpg 2x"
                    }
                  }
                ]
              },
              {
                "element": "div",
                "mediaQueries": [
                  {
                    "query": "max-width: 768px",
                    "style": "flex-direction: column; gap: 24px; max-width: 500px; max-width: unset;"
                  }
                ],
                "style": "border-block: 1px solid rgba(51, 50, 61, 0.15); display: flex; flex-direction: column; gap: 24px; justify-content: center; max-width: 350px; min-height: 100%; padding-block: 32px 50px;",
                "children": [
                  {
                    "element": "section",
                    "style": "display: flex; flex-direction: column; gap: 28px;",
                    "children": [
                      {
                        "element": "h2",
                        "attributes": {
                          "class": "title"
                        },
                        "text": "cwrapArray[Manage,Bookmark,Insure,Fylo]"
                      },
                      {
                        "element": "p",
                        "attributes": {
                          "class": "description"
                        },
                        "text": "cwrapArray[cwrapGlobal[manageDescription],cwrapGlobal[bookmarkDescription],cwrapGlobal[insureDescription],cwrapGlobal[fyloDescription]]"
                      }
                    ]
                  },
                  {
                    "element": "a",
                    "text": "GO TO PORTFOLIO",
                    "attributes": {
                      "class": "button2",
                      "href": "cwrapArray[manage,bookmark,insure,fylo]"
                    }
                  }
                ]
              }
            ]
          }
        }
      ]
    }
  ]
}